    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chartist": "0.11.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/moo": "^0.5.4",
